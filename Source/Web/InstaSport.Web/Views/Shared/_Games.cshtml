@model IEnumerable<InstaSport.Web.ViewModels.Games.UpcomingGameViewModel>

<div class="container">
    @foreach (var game in Model)
    {
        <div class="col-md-4">
            <div class="panel panel-info">
                <div class="panel-body">
                    @Html.DisplayFor(m => game)
                </div>
            </div>
        </div>
    }
</div>
